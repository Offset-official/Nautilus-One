<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <SequenceStar>
            <Condition ID="Elapsed" seconds="15"/>
            <Action ID="Descend" target_depth="-0.0"/>
        </SequenceStar>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="Descend">
            <input_port name="target_depth" type="double"/>
        </Action>
        <Condition ID="Elapsed">
            <input_port name="seconds" type="int"/>
        </Condition>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
