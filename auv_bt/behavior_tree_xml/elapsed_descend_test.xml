<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <SequenceStar>
            <Condition ID="Elapsed" seconds="15"/>
            <Action ID="Descend" target_depth="-0.5"/>
        </SequenceStar>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="Descend">
            <input_port default="0.0" name="target_depth">depth to descend to</input_port>
        </Action>
        <Condition ID="Elapsed">
            <input_port name="seconds" type="int"/>
        </Condition>
        <Action ID="Turn">
            <input_port default="0.5" name="speed">speed at which to turn</input_port>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
